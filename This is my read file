# Day5
1,for the given json  iterate overall for loops (for, for in, for of, for each)

const data = {
  name: "John",
  age: 25,
  city: "New York"
};

// for loop
console.log("Using for loop:");
for (let key in data) {
  console.log(`${key}: ${data[key]}`);
}

// for...in loop
console.log("\nUsing for...in loop:");
for (let key in data) {
  if (data.hasOwnProperty(key)) {
    console.log(`${key}: ${data[key]}`);
  }
}

// for...of loop (for arrays)
console.log("\nUsing for...of loop:");
const arr = [1, 2, 3, 4, 5];
for (let element of arr) {
  console.log(element);
}

// forEach loop (for arrays)
console.log("\nUsing forEach loop:");
arr.forEach((element) => {
  console.log(element);
});

